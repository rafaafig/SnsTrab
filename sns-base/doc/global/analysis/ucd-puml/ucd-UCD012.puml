@startuml
'skinparam monochrome true
'skinparam shadowing false

class Vaccine {
    +Id: int
    +Name: string
    +Brand: string
    +Type: VaccineType
}

class VaccineType {
    +Code: string
    +Description: string
}

class Lot {
    +Number: string
    +ExpirationDate: date
}

class VaccinationAppointment {
    +Id: int
    +SNSUser: SNSUser
    +VaccinationCenter: VaccinationCenter
    +Date: date
    +Time: time
    +Vaccine: Vaccine
}

class CheckIn {
    +Id: int
    +SNSUser: SNSUser
    +VaccinationAppointment: VaccinationAppointment
}

class VaccinationCall {
    +Id: int
    +Nurse: Nurse
    +SNSUser: SNSUser
}

class VaccineAdministration {
    +Id: int
    +Nurse: Nurse
    +SNSUser: SNSUser
    +Vaccine: Vaccine
    +Lot: Lot
}

class VaccinationRegist {
    +Id: int
    +Nurse: Nurse
    +SNSUser: SNSUser
    +Vaccine: Vaccine
    +Lot: Lot
}

class Administrator {
    +Id: int
    +Username: string
    +Password: string
}

class Receptionist {
    +Id: int
    +Username: string
    +Password: string
}

class Nurse {
    +Id: int
    +Username: string
    +Password: string
}

class SNSUser {
    +Id: int
    +Name: string
    +BirthDate: date
    +Sex: string
    +PostalAddress: string
    +CitizenCardNumber: string
}

class VaccinationCenter {
    +Id: int
    +Name: string
    +Address: string
    +PhoneNumber: string
    +Email: string
    +FaxNumber: string
    +Website: string
    +OpeningHours: string
    +ClosingHours: string
    +MaxVaccinesPerHour: int
}

class WaitingRoom {
    +Id: int
    +Capacity: int
}

class RecoveryRoom {
    +Id: int
    +Capacity: int
    +RecoveryPeriod: int
}

class AdverseReactionRecord {
    +Id: int
    +Nurse: Nurse
    +SNSUser: SNSUser
    +Description: string
    +Timestamp: datetime
}

class EUCOVIDDigitalCertificate {
    +Id: int
    +SNSUser: SNSUser
}

class SNSUserRecord {
    +Id: int
    +SNSUser: SNSUser
}

VaccinationAppointment --|> SNSUser
CheckIn --|> SNSUser
VaccinationCall --|> SNSUser
VaccineAdministration --|> SNSUser
VaccinationRegist --|> SNSUser
VaccinationAppointment --|> VaccinationCenter
CheckIn --|> VaccinationAppointment
VaccinationCall --|> Nurse
VaccineAdministration --|> Vaccine
VaccinationRegist --|> Vaccine
Administrator --|> VaccinationCenter
Receptionist --|> VaccinationCenter
Nurse --|> VaccinationCenter
Nurse --|> VaccineAdministration
Nurse --|> AdverseReactionRecord
SNSUser --|> EUCOVIDDigitalCertificate
SNSUserRecord --|> SNSUser

@enduml