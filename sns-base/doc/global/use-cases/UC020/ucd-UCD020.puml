@startuml
skinparam monochrome false
skinparam shadowing false

title UC020 - Register SNS User

class Receptionist {
    +Username: String [pk]
    +Password: String
}

class SNSUser {
    +Name: String
    +BirthDate: Date
    +Sex: String
    +PostalAddress: String
    +PhoneNumber: String
    +EmailAddress: String
    +CitizenCardNumber: String [pk2]
